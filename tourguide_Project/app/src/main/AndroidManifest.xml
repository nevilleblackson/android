<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.gebruiker.tourguide_project">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".EvenementenActivity"
            android:label="@string/evenementen"
            android:parentActivityName=".MainActivity"/>
        <activity
            android:name=".StadActivity"
            android:label="@string/stad"
            android:parentActivityName=".MainActivity"/>
        <activity android:name=".WinkelActivity"
            android:label="@string/winkels"
            android:parentActivityName=".MainActivity"/>/>
    </application>

</manifest>