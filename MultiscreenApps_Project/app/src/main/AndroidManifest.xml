<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.gebruiker.multiscreenapps_project">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Almere"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".EvenementenActivity"
            android:label="Evenementen"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".StadActivity"
            android:label="Stad"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".WinkelActivity"
            android:label="Winkels"
            android:parentActivityName=".MainActivity" />
        />
        <activity
            android:name=".WeerActivity"
            android:label="weer"
            android:parentActivityName=".MainActivity"/>
    </application>

</manifest>